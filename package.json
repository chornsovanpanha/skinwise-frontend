{
  "name": "skin-wise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cp .env.development .env && next dev --turbopack",
    "build": "next build",
    "build-dev": "cp .env.development .env.local && next build",
    "start": "cp .env.development .env.local && next start",
    "start-dev": "cp .env.development .env.local && next start",
    "migrate-dev": "cp .env.development .env && npx prisma migrate dev --name alter-user-search-table-dev",
    "migrate-prod": "cp .env.production .env && npx prisma migrate dev --name alter-user-search-table-prod",
    "prisma:dev": "cp .env.development .env && pnpx prisma migrate dev && pnpx prisma generate",
    "prisma:prod": "cp .env.production .env && pnpx prisma migrate deploy && pnpx prisma generate",
    "prisma-studio-dev": "cp .env.development .env &&  pnpx prisma studio",
    "prisma-studio-prod": "cp .env.production .env &&  pnpx prisma studio",
    "postinstall": "prisma generate",
    "prisma-seed-dev": "cp .env.development .env && npx prisma db seed",
    "prisma-seed-prod": "cp .env.production .env && npx prisma db seed",
    "prisma-seed-test": "cp .env.test .env && npx prisma db seed",
    "reset-dev": "cp .env.development .env && npx prisma migrate reset",
    "reset-prod": "cp .env.production .env && npx prisma migrate reset && npx prisma db seed",
    "stripe-listen": "stripe listen --forward-to localhost:3000/api/webhook",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@firebase/performance": "^0.7.8",
    "@google/genai": "^1.21.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons": "^3.34.1",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "dotenv": "^17.2.1",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "ioredis": "^5.8.1",
    "jose": "^6.1.0",
    "jotai": "^2.12.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "nextjs-toploader": "^3.9.17",
    "nodemailer": "^7.0.5",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
